{
  "name": "aidoc",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered healthcare consultation platform",
  "keywords": ["healthcare", "ai", "consultation", "nextjs", "gemini"],
  "author": "AIDOC Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "prod:build": "npm run db:generate && npm run build",
    "prod:start": "npm run db:migrate && npm run start",
    "deploy:vercel": "vercel --prod",
    "deploy:docker": "docker build -t aidoc . && docker run -p 3000:3000 aidoc"
  },
